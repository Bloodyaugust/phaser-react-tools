{
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/jsdoc-template"
  },
  "recurseDepth": 10,
  "source": {
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["README.md", "src"]
  },
  "templates": {
    "referenceTitle": "phaser-react-tools"
  }
}
